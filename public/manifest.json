{
  "manifest_version": 3,
  "name": "Liberate - 0L Browser Extension Wallet ",
  "version": "1.0.0",
  "host_permissions": ["storage"],
  "background": {
    "service_worker": "build/background.js"
  },
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["build/injection.js"]
    }
  ]
}
